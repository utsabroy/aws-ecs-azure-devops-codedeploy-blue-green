{
  "applicationName": "CODE_DEPLOY_APPLICATION_NAME",
  "deploymentGroupName": "CODE_DEPLOY_GROUP_NAME",
  "revision": {
    "revisionType": "S3",
    "s3Location": {
      "bucket": "blue-green-azure-demo",
      "key": "BUILD_ID/appspec.yaml",
      "bundleType": "YAML"
    }
  },
  "ignoreApplicationStopFailures": false,
  "autoRollbackConfiguration": {
    "enabled": true,
    "events": [
      "DEPLOYMENT_FAILURE",
      "DEPLOYMENT_STOP_ON_ALARM",
      "DEPLOYMENT_STOP_ON_REQUEST"
    ]
  }
}
